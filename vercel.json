{
    "version": 2,
    "builds": [
      {
        "src": "api/dist/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }